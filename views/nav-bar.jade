div#nav-bar.nav
  if(fullname)
    if view == 'home' 
      div#back.hidden.block
        a(href='#{back}')
          i.fa.fa-arrow-left
          div.nav-title BACK
    else
      div#back.block
        a(href='#{back}')
          i.fa.fa-arrow-left
          div.nav-title BACK
  if Version == 'A' 
    div.title.version_a #{title}
  else
    div.title.version_b #{title}
  if(fullname)
    div#settings.block
      div
        i.fa.fa-bars
        div.nav-title MENU